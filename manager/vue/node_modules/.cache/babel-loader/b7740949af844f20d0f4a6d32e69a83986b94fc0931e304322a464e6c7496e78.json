         this.goodsData = res.data\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleClick(tab, event) {\r\n      this.activeName = tab.name\r\n    },\r\n    navTo(url) {\r\n      location.href = url\r\n    },\r\n    collectGoods() {\r\n      let data = {userId: this.user.id, goodsId: this.goodsId, businessId: this.goodsData.businessId}\r\n      this.$request.post('collect/add', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('收藏成功')\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    addCart() {\r\n      let data = {num: 1, userId: this.user.id, goodsId: this.goodsId, businessId: this.goodsData.businessId}\r\n      this.$request.post('/cart/add', data).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success('加入购物车成功')\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>"],"mappings":"AA4DA,OAAAA,KAAA;AAEA;EACAC,QAAA;IACAD,MAAA;MACA,OAAAA,KAAA;IACA;EACA;EACAE,KAAA;IACA,IAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAR,OAAA,EAAAA,OAAA;MACAS,SAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;EACA;EACAC,OAAA;IACAD,UAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,+BAAAf,OAAA,EAAAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,SAAA,GAAAQ,GAAA,CAAAlB,IAAA;QACA;UACA,KAAAoB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,YAAAC,GAAA,EAAAC,KAAA;MACA,KAAAd,UAAA,GAAAa,GAAA,CAAAE,IAAA;IACA;IACAC,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAG,aAAA;MACA,IAAA/B,IAAA;QAAAgC,MAAA,OAAA3B,IAAA,CAAAD,EAAA;QAAAH,OAAA,OAAAA,OAAA;QAAAgC,UAAA,OAAAvB,SAAA,CAAAuB;MAAA;MACA,KAAAlB,QAAA,CAAAmB,IAAA,gBAAAlC,IAAA,EAAAiB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,QAAA,CAAAe,OAAA;QACA;UACA,KAAAf,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAc,QAAA;MA