{"ast":null,"code":"export default {\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      goodsData: [],\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0\n    };\n  },\n  mounted() {\n    this.loadGoods(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    navTo(url) {\n      location.href = url;\n    },\n    loadGoods(pageNum) {\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/collect/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.collectData = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    del(id) {\n      // 单个删除\n      this.$confirm('您确定删除吗？', '确认删除', {\n        type: \"warning\"\n      }).then(response => {\n        this.$request.delete('/collect/delete/' + id).then(res => {\n          if (res.code === '200') {\n            // 表示操作成功\n            this.$message.success('操作成功');\n            this.loadCollect(1);\n          } else {\n            this.$message.error(res.msg); // 弹出错误的信息\n          }\n        });\n      }).catch(() => {});\n    },\n    handleCurrentChange(pageNum) {\n      this.loadCollect(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["data","user","JSON","parse","localStorage","getItem","goodsData","pageNum","pageSize","total","mounted","loadGoods","methods","navTo","url","location","href","$request","get","params","then","res","code","collectData","list","$message","error","msg","del","id","$confirm","type","response","delete","success","loadCollect","catch","handleCurrentChange"],"sources":["src/views/front/Cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <div\r\n        style=\"width: 70%; background-color: white; margin: 30px auto; border-radius: 20px;\">\r\n      <div style=\"padding-bottom: 10px;\">\r\n        <div style=\"font-size: 18px; color: black; line-height: 80px; border-bottom: #cccccc 1px solid;\">\r\n          <div style=\"margin-left: 20px;\">购物车（共 {{ goodsData.length }} 件）</div>\r\n        </div>\r\n        <div style=\"margin: 20px 0; padding: 0 50px;\">\r\n          <div class=\"table\">\r\n            <el-table :data=\"collectData\" strip>\r\n              <el-table-column label=\"商品图片\" width=\"120px\">\r\n                <template v-slot=\"scope\">\r\n                  <div style=\"display: flex; align-items: center\">\r\n                    <el-image style=\"width: 80px; height: 60px; border-radius: 3px\" v-if=\"scope.row.goodsImg\"\r\n                              :src=\"scope.row.goodsImg\" :preview-src-list=\"[scope.row.goodsImg]\"></el-image>\r\n                  </div>\r\n                </template>\r\n              </el-table-column>\r\n              <el-table-column prop=\"goodsName\" label=\"商品名称\" width=\"350px\">\r\n                <template v-slot=\"scope\">\r\n                  <a href=\"#\" @click=\"navTo('/front/detail?id=' + scope.row.goodsId)\">{{ scope.row.goodsName }}</a>\r\n                </template>\r\n              </el-table-column>\r\n              <el-table-column prop=\"businessName\" label=\"店铺名称\">\r\n                <template v-slot=\"scope\">\r\n                  <a href=\"#\" @click=\"navTo('/front/business?id=' + scope.row.businessId)\">{{ scope.row.businessName }}</a>\r\n                </template>\r\n              </el-table-column>\r\n              <el-table-column prop=\"goodsPrice\" label=\"商品价格\"></el-table-column>\r\n              <el-table-column label=\"操作\" align=\"center\" width=\"180\">\r\n                <template v-slot=\"scope\">\r\n                  <el-button size=\"mini\" type=\"danger\" plain @click=\"del(scope.row.id)\">移除收藏</el-button>\r\n                </template>\r\n              </el-table-column>\r\n            </el-table>\r\n\r\n            <div class=\"pagination\" style=\"margin-top: 20px;\">\r\n              <el-pagination\r\n                  background\r\n                  @current-change=\"handleCurrentChange\"\r\n                  :current-page=\"pageNum\"\r\n                  :page-sizes=\"[5, 10, 20]\"\r\n                  :page-size=\"pageSize\"\r\n                  layout=\"total, prev, pager, next\"\r\n                  :total=\"total\">\r\n              </el-pagination>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      goodsData: [],\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadGoods(1)\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    navTo(url) {\r\n      location.href = url\r\n    },\r\n    loadGoods(pageNum) {\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/collect/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.collectData = res.data?.list\r\n          this.total = res.data?.total\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/collect/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.loadCollect(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.loadCollect(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>"],"mappings":"AAyDA;EAEAA,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,SAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;EACA;EACAC,OAAA;IACAC,MAAAC,GAAA;MACAC,QAAA,CAAAC,IAAA,GAAAF,GAAA;IACA;IACAH,UAAAJ,OAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAU,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAZ,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QACA;MACA,GAAAY,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAC,WAAA,GAAAF,GAAA,CAAArB,IAAA,EAAAwB,IAAA;UACA,KAAAf,KAAA,GAAAY,GAAA,CAAArB,IAAA,EAAAS,KAAA;QACA;UACA,KAAAgB,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;QACA;MACA;IACA;IACAC,IAAAC,EAAA;MAAA;MACA,KAAAC,QAAA;QAAAC,IAAA;MAAA,GAAAX,IAAA,CAAAY,QAAA;QACA,KAAAf,QAAA,CAAAgB,MAAA,sBAAAJ,EAAA,EAAAT,IAAA,CAAAC,GAAA;UACA,IAAAA,GAAA,CAAAC,IAAA;YAAA;YACA,KAAAG,QAAA,CAAAS,OAAA;YACA,KAAAC,WAAA;UACA;YACA,KAAAV,QAAA,CAAAC,KAAA,CAAAL,GAAA,CAAAM,GAAA;UACA;QACA;MACA,GAAAS,KAAA,QACA;IACA;IACAC,oBAAA9B,OAAA;MACA,KAAA4B,WAAA,CAAA5B,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}